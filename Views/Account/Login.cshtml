@{
    ViewBag.Title = "Login";
}

<h2>Login</h2>

@using (Html.BeginForm())
{
    <div>
        <label for="userName">User Name:</label>
        <input type="text" id="userName" name="userName" required />
    </div>
    <div>
        <input type="submit" value="Log In" />
    </div>
    @Html.ValidationSummary(true)
}
